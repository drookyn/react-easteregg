{
  "name": "react-easteregg",
  "version": "1.0.0",
  "description": "React easter egg component",
  "main": "index.js",
  "author": "Florian Vogt <npm@vogt-da.de>",
  "license": "MIT",
  "homepage": "https://github.com/drookyn/react-easteregg/issues",
  "bugs": {
    "url": "https://github.com/eeynard/react-easter-egg/issues"
  },
  "scripts": {
    "build": "babel ./src --ignore *.test.js --out-dir ./dist",    
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "prepublish": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "browserslist": "> 0.25%, not dead",
  "repository": {
    "type": "git",
    "url": "git@github.com:drookyn/react-easteregg.git"
  },
  "keywords": [
    "javascript",
    "reactjs",
    "react",
    "react-component",
    "component",
    "easter egg",
    "easter-egg",
    "cheatcode",
    "cheat"
  ],
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-transform-modules-umd": "7.1.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.0",
    "husky": "1.1.2",
    "lint-staged": "7.3.0"
  }
}
