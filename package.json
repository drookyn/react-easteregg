{
  "name": "react-easteregg",
  "version": "1.0.0",
  "description": "React easter egg component",
  "main": "index.js",
  "author": "Florian Vogt <npm@vogt-da.de>",
  "license": "MIT",
  "homepage": "https://github.com/drookyn/react-easteregg/issues",
  "bugs": {
    "url": "https://github.com/eeynard/react-easter-egg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:drookyn/react-easteregg.git"
  },
  "keywords": [
    "javascript",
    "reactjs",
    "react",
    "react-component",
    "component",
    "easter egg",
    "easter-egg",
    "cheatcode",
    "cheat"
  ],
  "scripts": {
    "build": "rimraf \"./dist\" && babel --ignore **/*.test.js ./src --out-dir ./dist",
    "lint": "eslint --ext .jsx --ext .js ./src",
    "lint:fix": "eslint --ext .jsx --ext .js --fix ./src",
    "prepublish": "npm run build",
    "prepublishOnly": "npm run lint && npm run test",
    "test": "jest --updateSnapshot --ci",
    "test:clear": "jest --clearCache",
    "test:watch": "jest --watch --updateSnapshot"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "dependencies": {
    "prop-types": "15.6.2",
    "react-event-listener": "0.6.4"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-transform-modules-umd": "7.1.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.0",
    "husky": "1.1.2",
    "jest": "23.6.0",
    "jsdom": "12.2.0",
    "lint-staged": "7.3.0",
    "regenerator-runtime": "0.12.1",
    "rimraf": "2.6.2",
    "sinon": "7.1.0"
  }
}
